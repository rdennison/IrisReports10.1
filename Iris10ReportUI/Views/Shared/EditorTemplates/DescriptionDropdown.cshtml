@model object

@{
    SelectList ColumnNameList = new SelectList((SelectList) Session["ReportColumnName"], "Value", "Text");
    @(Html.Kendo()
          .ComboBox()
          .Name("ReportFieldList")
          .Placeholder("Select...")
          .Filter("startswith")
          .Suggest(true)
          .SelectedIndex(0)
          .BindTo((SelectList) ColumnNameList))
}